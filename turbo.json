{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build:sanity": {
      "inputs": ["./studio/**"],
      "outputs": ["./static/studio/**"]
    },
    "build": {
      "dependsOn": ["build:sanity"],
      "outputs": ["./.svelte-kit", ".vercel_build_output", ".vercel/output"]
    }
  }
}
