    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.10",
    "autoprefixer": "10.4.16",
    "cssnano": "6.0.1",
    "mdsvex": "0.11.0",
    "postcss": "8.4.31",
    "postcss-load-config": "4.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "svelte": "4.2.1",
    "svelte-check": "3.5.2",
    "svelte-preprocess": "5.0.4",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
