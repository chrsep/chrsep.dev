{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev --host 0.0.0.0",
    "build": "rm -rf ./static/studio && mkdir ./static/studio && cp -R ../studio/dist/* ./static/studio && svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@poppanator/sveltekit-svg": "4.1.3",
    "@sveltejs/adapter-vercel": "1.0.0-next.53",
